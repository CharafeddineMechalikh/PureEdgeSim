<?xml version="1.0"?>
<applications>
	<application name="AUGMENTED_REALITY">
	    <rate>10</rate>
		<usage_percentage>20</usage_percentage> <!-- percentage of devices using this type of applications -->
		<latency>1</latency> <!-- latency in seconds -->
		<container_size>25000</container_size><!--application/container size in kilobytes -->
		<request_size>72000</request_size><!-- the offloading request that will be sent to the orchestrator and then to the device where the task will be offloaded in kilobytes -->
		<results_size>10000</results_size><!-- the results of the offlaoded task in kilobytes -->
		<task_length>5000</task_length><!--MI: million instructions --> 
	</application>
	<application name="E_HEALTH">
	    <rate>10</rate>
		<usage_percentage>20</usage_percentage> 
		<latency>200</latency>
		<container_size>39000</container_size>
		<request_size>60000</request_size>
		<results_size>60000</results_size>
		<task_length>60000</task_length> 
	</application> 
	<application name="HEAVY_COMP_APP">
	    <rate>5</rate>
		<usage_percentage>20</usage_percentage>
		<latency>1</latency> <!-- a great number represents no latency- sensitivity -->
		<container_size>9000</container_size>
		<request_size>25000</request_size>
		<results_size>25000</results_size>
		<task_length>30000</task_length> 
	</application> 
	<application name="infotainement"> 
		<rate>2</rate>
		<usage_percentage>30</usage_percentage> 
		<latency>300</latency> 
		<container_size>1000</container_size>
		<request_size>5000</request_size>
		<results_size>5000</results_size>
		<task_length>50000</task_length> 
	</application> 
</applications>